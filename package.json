{
  "name": "@raideno/auto-form",
  "version": "0.0.1",
  "description": "Generate forms automatically from Zod schemas with built-in validation and customizable field types",
  "type": "module",
  "main": "./dist/lib/index.cjs",
  "module": "./dist/lib/index.mjs",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.cjs"
    },
    "./styles.css": "./dist/lib/auto-form.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "keywords": [
    "react",
    "form",
    "zod",
    "validation",
    "auto-form",
    "schema",
    "typescript"
  ],
  "author": "raideno",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raideno/auto-form.git"
  },
  "bugs": {
    "url": "https://github.com/raideno/auto-form/issues"
  },
  "homepage": "https://github.com/raideno/auto-form#readme",
  "scripts": {
    "dev:lib": "vite --config vite.lib.config.ts",
    "dev:demo": "vite --config vite.demo.config.ts",
    "build:lib": "vite build --config vite.lib.config.ts",
    "build:demo": "vite build --config vite.demo.config.ts",
    "lint": "eslint .",
    "preview": "vite preview --config vite.demo.config.ts",
    "typecheck": "tsc --noEmit",
    "test:exports": "node test-exports.mjs"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/themes": "^3.2.1",
    "@raideno/auto-form": "^0.0.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lodash": "^4.17.21",
    "lucide-react": "^0.540.0",
    "react-error-boundary": "^6.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.85.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@hookform/resolvers": "^5.2.1",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-hook-form": "^7.62.0",
    "zod": "^4.0.17"
  }
}
